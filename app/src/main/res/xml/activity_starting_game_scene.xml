<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:id="@+id/btn_start_game"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/cardView3"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="@+id/cardView3" />
        <Constraint
            android:id="@+id/btn_start_game"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/cardView3"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="@+id/cardView3" />
        <Constraint
            android:id="@+id/tv_game_title"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/textView5"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/cardView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/cardView2"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="@+id/cardView2"
            motion:layout_constraintTop_toBottomOf="@+id/cardView2" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/cardView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="@+id/cardView2"
            android:id="@+id/cardView3"
            motion:layout_constraintTop_toBottomOf="@+id/cardView2" />
        <Constraint
            android:id="@+id/textView5"
            motion:layout_constraintEnd_toEndOf="@+id/tv_game_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintStart_toStartOf="@+id/tv_game_title"
            motion:layout_constraintBottom_toTopOf="@+id/cardView4"
            motion:layout_constraintTop_toBottomOf="@+id/tv_game_title" />
        <Constraint
            android:id="@+id/cardView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="44dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toTopOf="@+id/cardView2"
            motion:layout_constraintTop_toBottomOf="@+id/textView5"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/cardView2"
            motion:layout_constraintEnd_toEndOf="@+id/cardView4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/cardView4"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toTopOf="@+id/cardView3"
            motion:layout_constraintVertical_bias="0.727"
            motion:layout_constraintTop_toBottomOf="@+id/cardView4"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>